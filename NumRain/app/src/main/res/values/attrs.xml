<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="lightColor" format="color" />
    <attr name="startOffset" format="integer" />
    <attr name="normalColor" format="color" />
    <attr name="textSize" format="dimension" />

    <declare-styleable name="NumRain">
        <attr name="textSize" />
        <attr name="lightColor" />
        <attr name="normalColor" />
    </declare-styleable>

    <declare-styleable name="NumRainItem">
        <attr name="startOffset" />
        <attr name="textSize" />
        <attr name="lightColor" />
        <attr name="normalColor" />
    </declare-styleable>
</resources>
